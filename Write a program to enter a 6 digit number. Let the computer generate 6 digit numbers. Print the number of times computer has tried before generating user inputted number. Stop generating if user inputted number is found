import random
k=1
while k>0:
    n=int(input("Enter a 6 digit number: "))
    i=1
    count=0
    if (len(str(n))==6):
        while i>0:
            comp=random.randint(-999999,999999)
            if comp!=n:
                count+=1
            else:
                print("Matched. The number is " , comp)
                break
        print("Computer Generated {} numbers: " .format(count)
    else:
        print("Please enter a 6 digit number")
        k=1
